{
    "connector.class": "io.debezium.connector.oracle.OracleConnector",
    "tasks.max": "1",
    "topic.prefix": "corebanking",
    "database.hostname": "host.docker.internal",
    "database.port": "1521",
    "database.user": "C##CDC",
    "database.password": "cdcpass",
    "database.dbname": "XEPDB1",
    "database.pdb.name": "XEPDB1",
    "database.url": "jdbc:oracle:thin:@host.docker.internal:1521/XEPDB1",

    "table.include.list": "C##CDC.CUSTOMERS,C##CDC.ORDERS",
    "snapshot.mode": "initial",
    "log.mining.strategy": "online_catalog",

    "schema.history.internal.kafka.bootstrap.servers": "kafka:9092",
    "bootstrap.servers": "kafka:9092",
    "schema.history.internal.kafka.topic": "corebanking.cdc.schema-history",
    "schema.history.internal.kafka.consumer.group.id": "corebanking-oracle-cdc-group",
    "schema.history.internal.kafka.recovery.poll.interval.ms": 700000,

    "key.converter": "org.apache.kafka.connect.json.JsonConverter",
    "value.converter": "org.apache.kafka.connect.json.JsonConverter",
    "key.converter.schemas.enable":  "false",
    "value.converter.schemas.enable": "false",

    "decimal.handling.mode": "string",

    "transforms": "routeSchema,routeCustomers",

    "transforms.routeSchema.type": "org.apache.kafka.connect.transforms.RegexRouter",
    "transforms.routeSchema.regex": "^corebanking$",
    "transforms.routeSchema.replacement": "corebanking-ddl",

    "transforms.routeCustomers.type": "org.apache.kafka.connect.transforms.RegexRouter",
    "transforms.routeCustomers.regex": "^corebanking\\.C__CDC\\.CUSTOMERS$",
    "transforms.routeCustomers.replacement": "corebanking.customers"
}
